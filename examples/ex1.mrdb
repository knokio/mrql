-- select mean(age) as res from stuff 
export media_stuff

def_query media_stuff():
   def table = stuff

   def map(row):
      {"count":1,"sum":row.age}

   def reduce(a,b):
      {"sum":a.sum + b.sum, "count":a.count + b.count}

   def finalize(final_row):
      {"res": final_row.sum / final_row.count}
